<?xml version="1.0"?> 
<cross-domain-policy>
	<allow-access-from domain="*.163.com"/>
	<allow-access-from domain="*.126.com"/>
	<allow-access-from domain="*.126.net"/>
	<allow-access-from domain="*.yeah.net"/>
	<allow-access-from domain="*.188.com" />
	<allow-access-from domain="*.lofter.com" />
	<allow-http-request-headers-from domain="*.163.com" headers="*"/>
	<allow-http-request-headers-from domain="*.126.com" headers="*"/>
	<allow-http-request-headers-from domain="*.126.net" headers="*"/>
	<allow-http-request-headers-from domain="*.yeah.net" headers="*"/>
	<allow-http-request-headers-from domain="*.188.com" headers="*" />
	<allow-http-request-headers-from domain="*.lofter.com" headers="*" />
	<site-control permitted-cross-domain-policies="master-only"/>
</cross-domain-policy>
